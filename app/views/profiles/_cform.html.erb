<%= simple_form_for @profile, url: profiles_path  do |f| %>

<%= f.error_notification %>
<%= f.input :firstname%>
<%= f.input :lastname%>
<%= f.input :age%>
<%= f.input :type, :as => :hidden, :input_html => { :value => "Cprofile" } %>

<%= f.button :submit, "Submit" %>
<% if @edit %>
 | <%= link_to "back", account_path, class: "btn btn-danger" %>
<% end %>

<% end %>